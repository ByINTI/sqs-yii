    "type": "yii2-extension",
    "keywords": ["yii", "byinti", "sqs"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Daniel Resnitzky",
            "email": "dcresnitzky@gmail.com"
        }
    ],
